					= render "searches/home_listings"
	#navbar
		= image_tag "/assets/navbar2.png", id: "navimage"
		.container#navlinks
			.span-11.prepend-11
				%ul.front_end_nav
					%li
						= link_to strains_path do
							= image_tag "/assets/field-logo.png", alt: "canibus dispensaries"
