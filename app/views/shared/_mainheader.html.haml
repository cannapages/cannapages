							= image_tag "/assets/news-icon.png", alt: "weed reviews"
						= link_to critiques_home_path do
							= image_tag "/assets/critiques-icon.png", alt: "weedmaps"
						= link_to cadets_home_path do
							= image_tag "/assets/members-icon.png", alt: "best marijuna"
						= link_to forums_path do
							= image_tag "/assets/forum-icon.png", alt: "canibus dispensaries"
