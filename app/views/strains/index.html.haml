#top
.span-18.first.strain_container
	.ribbon-wrapper
		.ribbon-front
			%h2 Field Guide
		.ribbon-edge-topleft
		.ribbon-edge-topright
		.ribbon-edge-bottomleft
		.ribbon-edge-bottomright
		.ribbon-back-left
		.ribbon-back-right
	#field_results
		- @strains.first(10).each do |strain|
			.result
				= image_tag "http://placehold.it/150x150"
				%h2= ("#{strain.name}")
				%p= "#{strain.dominance}"
				%p= "#{truncate strain.bio, length: 150}"